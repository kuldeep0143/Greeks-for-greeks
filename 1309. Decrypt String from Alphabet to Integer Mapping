class Solution {
    public String freqAlphabets(String s) {
   String ans = "";
        char[] alphabets = new char[26];
        for(int i = 0 ; i < 26 ; i++)
        {
            alphabets[i] = (char)('a'+i) ;
        }
        int n = 0 ;
        for( int i = 0 ; i < s.length() ; i++)
        {
            char ch = s.charAt(i);
            if( s.length() > i+2 && s.charAt(i+2) == '#' )
            {
                n = Integer.parseInt(s.substring(i,i+2));
                ans += alphabets[n-1] ;
                i += 2 ;
            }
            else
            {
                n = Integer.parseInt(""+ch);
                ans += alphabets[n-1];
            }
        }
        return ans ;
    }
}
